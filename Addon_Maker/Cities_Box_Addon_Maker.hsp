		stop
	}
	
	clrobj
	color 50, 50, 50 : boxf
	
	/*戻るボタンとページタイトル*/
		objimage 5
		if Addon_Type = "road" or Addon_Type = "railroad" {
			pos 10, 10 : objsize 32, 32
			
			if road_img_type = "Default" or road_img_type = "Train_Crossing" {
				button "", *make_road_3
			}
			if road_img_type = "Dead_End" {
				button "", *make_road_4
			}
			if road_img_type = "Intersection_T" {
				button "", *make_road_5
			}
			if road_img_type = "Turn" {
				button "", *make_road_6
			}
			if road_img_type = "Intersection_Cross" or road_img_type = "Under_Construction" {
				button "", *make_road_7
			}
			
